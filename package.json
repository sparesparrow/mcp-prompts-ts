{
  "name": "@sparesparrow/mcp-prompts",
  "version": "3.0.1",
  "description": "A standalone server for managing and serving prompts for the Model Context Protocol (MCP)",
  "main": "dist/index.mjs",
  "type": "module",
  "bin": {
    "mcp-prompts": "dist/index.mjs"
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sparesparrow/mcp-prompts.git"
  },
  "homepage": "https://github.com/sparesparrow/mcp-prompts#readme",
  "bugs": {
    "url": "https://github.com/sparesparrow/mcp-prompts/issues"
  },
  "scripts": {
    "test": "bash scripts/run-tests.sh --unit",
    "lint": "eslint \"src/**/*.ts\" && prettier --check .",
    "validate:prompts": "ts-node src/scripts/validate-prompts.ts",
    "build": "node build.mjs",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "clean": "rimraf dist",
    "start": "node dist/scripts/start-server.js",
    "dev": "ts-node --esm src/scripts/start-server.ts",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=\"tests/unit|src/__tests__\" --detectOpenHandles --forceExit",
    "test:build": "npm run build && node dist/index.js --version",
    "test:inspector": "npx @modelcontextprotocol/inspector node dist/index.js",
    "docker": "bash docker/scripts/docker-compose-manager.sh",
    "setup:claude": "scripts/setup-claude-desktop.sh",
    "publish:patch": "./scripts/publish.sh patch",
    "publish:minor": "./scripts/publish.sh minor",
    "publish:major": "./scripts/publish.sh major",
    "release:patch": "./scripts/release.sh --version patch --publish",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "type-check": "tsc --noEmit",
    "workflow:run:file": "ts-node src/scripts/workflow-cli.ts workflow run",
    "workflow:save:file": "ts-node src/scripts/workflow-cli.ts workflow save",
    "workflow:run:id": "ts-node src/scripts/workflow-cli.ts workflow run",
    "docs:generate": "typedoc --out docs/api src/index.ts",
    "test:integration": "bash scripts/run-tests.sh --integration"
  },
  "keywords": [
    "mcp",
    "prompt",
    "template",
    "llm"
  ],
  "author": "sparesparrow",
  "license": "MIT",
  "dependencies": {
    "@elastic/elasticsearch": "^9.0.2",
    "@modelcontextprotocol/sdk": "^1.13.2",
    "@modelcontextprotocol/server-postgres": "0.6.2",
    "@sparesparrow/mcp-prompts-catalog": "^0.1.0",
    "@sparesparrow/mcp-prompts-contracts": "^0.1.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "eventsource": "^4.0.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "node-fetch": "^2.7.0",
    "pg": "^8.16.2",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "proper-lockfile": "^4.1.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "ws": "^8.18.2",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/estree": "^1.0.8",
    "@types/eventsource": "^1.1.15",
    "@types/express": "^5.0.3",
    "@types/glob": "^8.1.0",
    "@types/handlebars": "^4.1.0",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^30.0.0",
    "@types/json-schema": "^7.0.15",
    "@types/node-fetch": "^2.6.12",
    "@types/param-case": "^0.0.29",
    "@types/pg": "^8.15.4",
    "@types/pino": "^7.0.4",
    "@types/proper-lockfile": "^4.1.4",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "esbuild": "^0.25.5",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsdoc": "^51.0.3",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "husky": "^8.0.3",
    "ioredis": "^5.6.1",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "shx": "^0.4.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.5",
    "typescript": "^5.4.5"
  }
}
